<configuration>
    <appender name="STDOUT" class="io.polyapi.knative.function.log.PolyAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] [%level]%msg[/%level]%n</pattern>
        </encoder>
    </appender>

    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>